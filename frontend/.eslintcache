nChange={handleProductChange}\r\n                defaultValue={\"\"}\r\n                value={product.value}\r\n                placeholder=\"Product\"\r\n              />\r\n            </Col>\r\n            <Form.Label column sm={1}>\r\n              Stock\r\n            </Form.Label>\r\n            <Col sm={3}>\r\n              <Form.Control\r\n                size=\"sm\"\r\n                type=\"text\"\r\n                placeholder=\"Stock\"\r\n                value={prod.countInStock ? prod.countInStock.balanceQty : 0}\r\n                disabled\r\n              />\r\n            </Col>\r\n          </Form.Group>\r\n\r\n          <Form.Group as={Row}>\r\n            <Form.Label column sm={1}>\r\n              Carrying\r\n            </Form.Label>\r\n            <Col sm={3}>\r\n              <Form.Control\r\n                size=\"sm\"\r\n                type=\"number\"\r\n                placeholder=\"Shipping Cost\"\r\n                value={shippingCost}\r\n                onChange={(e) => {\r\n                  setShippingCost(e.target.value);\r\n                }}\r\n              />\r\n            </Col>\r\n            <Form.Label column sm={1}>\r\n              Suppliers\r\n            </Form.Label>\r\n            <Col sm={3}>\r\n              <Select\r\n                options={supplierOptions}\r\n                onChange={(supplier) => setSupplier(supplier.value)}\r\n                defaultValue={supplierOptions[2]}\r\n                value={supplier.value}\r\n                placeholder=\"Supplier\"\r\n              />\r\n            </Col>\r\n            <Form.Label column sm={1}>\r\n              StockPrice\r\n            </Form.Label>\r\n            <Col sm={3}>\r\n              <Form.Control\r\n                disabled\r\n                size=\"sm\"\r\n                type=\"number\"\r\n                placeholder=\"Current Price\"\r\n                value={prod.price ? prod.price[prod.price - 1].price : 0}\r\n              />\r\n            </Col>\r\n          </Form.Group>\r\n\r\n          <Form.Group as={Row}>\r\n            <Form.Label column sm={1}>\r\n              Quantity\r\n            </Form.Label>\r\n            <Col sm={3}>\r\n              <Form.Control\r\n                size=\"sm\"\r\n                type=\"number\"\r\n                placeholder=\"Quantity\"\r\n                value={quantity}\r\n                onChange={(e) => setQuantity(e.target.value)}\r\n              />\r\n            </Col>\r\n            <Form.Label column sm={1}>\r\n              Price\r\n            </Form.Label>\r\n            <Col sm={3}>\r\n              <Form.Control\r\n                size=\"sm\"\r\n                type=\"number\"\r\n                placeholder=\"Price\"\r\n                value={price}\r\n                onChange={(e) => setPrice(e.target.value)}\r\n              />\r\n            </Col>\r\n            <Form.Label column sm={1}>\r\n              Unit\r\n            </Form.Label>\r\n            <Col sm={3}>\r\n              <Select\r\n                value={unit.value}\r\n                onChange={(unit) => setUnit(unit.value)}\r\n                options={unitOptions}\r\n                defaultValue={unitOptions[2]}\r\n                placeholder=\"Unit\"\r\n              />\r\n            </Col>\r\n          </Form.Group>\r\n\r\n          <Form.Group as={Row}>\r\n            <Col sm={4}></Col>\r\n            <Col sm={4}>\r\n              <Button\r\n                type=\"submit\"\r\n                size=\"sm\"\r\n                variant=\"info\"\r\n                className=\"rounded\"\r\n              >\r\n                Save to Stock\r\n              </Button>\r\n            </Col>\r\n          </Form.Group>\r\n        </Form>\r\n      </div>\r\n      {/* {Arr.length < 1 ? null : <TableAddPurchase setArr={setArr} arr={Arr} />} */}\r\n\r\n      {/* Table for showing all of the purchases */}\r\n      <div className=\"card mt-3\">\r\n        <PurchaseTable\r\n          loading={loading}\r\