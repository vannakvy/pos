nt,\r\n} from \"../../actions/eBookActions/eBookContentActions\";\r\nimport {getLanguages} from \"../../actions/eBookActions/eBookCourseActions\";\r\nimport {getDetailByContentId} from \"../../actions/eBookActions/eBookDetailActions\";\r\nimport { FaTrash, FaEdit, FaExternalLinkAlt } from \"react-icons/fa\";\r\nimport { Link } from \"react-router-dom\";\r\nimport Loader from \"../../components/Loader\";\r\n\r\nconst AddminContentScreen = () => {\r\n  // const history = useHistory();\r\n  const dispatch = useDispatch();\r\n  const [contentId, setContentId] = useState(\"\");\r\n  const [title, setTitle] = useState(\"\");\r\n  const [section, setSection] = useState(\"\");\r\n  const [langId, setLangId] = useState(\"languages\");\r\n  const { courses,loading, error } = useSelector((state) => state.courses);\r\n  const contentCreate = useSelector((state) => state.contentCreate);\r\n  const { content} = useSelector((state) => state.contentsByLang);\r\n  const contentDelete = useSelector((state) => state.contentDelete);\r\n  const contentUpdate = useSelector((state) => state.contentUpdate);\r\n\r\n\r\n  const handleSubmit = (e) => {\r\n    e.preventDefault();\r\n    if (title !== \"\" && section !== \"\" && langId !== \"languages\") {\r\n      if (contentId !== \"\") {\r\n        dispatch(updateContent(contentId, title, section));\r\n      } else {\r\n        dispatch(addContent(title, section, langId));\r\n      }\r\n    }\r\n\r\n    setTitle(\"\");\r\n    setSection(\"\");\r\n  };\r\n\r\n  const changeId = (id) => {\r\n    setLangId(id);\r\n  };\r\n  useEffect(() => {\r\n    dispatch(getLanguages());\r\n    dispatch(getContent(langId));\r\n  }, [langId, contentCreate, contentDelete, contentUpdate]);\r\n\r\n  return (\r\n    <div className=\"content\">\r\n      <div className=\"container\">\r\n        <div className=\"row\">\r\n          <div className=\" col-md-6 \">\r\n            <div className=\" mb-3\">\r\n              <select\r\n                className=\"custom-select\"\r\n                onChange={(e) => changeId(e.target.value)}\r\n                value={langId}\r\n              >\r\n                <option value={langId}>{langId}</option>\r\n                {courses &&\r\n                  courses.map((course) => (\r\n                    <option value={course.title} key={course._id}>\r\n                      {course.title}\r\n                    </option>\r\n                  ))}\r\n              </select>\r\n            </div>\r\n          </div>\r\n          <div className=\" col-md-6 \">\r\n            <button\r\n              className=\"btn btn-primary mb-2\"\r\n              type=\"button\"\r\n              data-toggle=\"collapse\"\r\n              data-target=\"#contentControl\"\r\n              aria-expanded=\"false\"\r\n              aria-controls=\"contentControl\"\r\n              onClick={() => setContentId(\"\")}\r\n            >\r\n              Create a Content\r\n            </button>\r\n          </div>\r\n        </div>\r\n\r\n        <form onSubmit={handleSubmit}>\r\n          <div className=\"collapse\" id=\"contentControl\">\r\n            <div className=\"languageForm mb-2 w-50 border  pb-3\">\r\n              <div className=\"container \">\r\n                <div className=\"form-group\">\r\n                  <label htmlFor=\"title\">Title</label>\r\n                  <input\r\n                    onChange={(e) => setTitle(e.target.value)}\r\n                    type=\"text\"\r\n                    className=\"form-control\"\r\n                    id=\"title\"\r\n                    value={title}\r\n                  />\r\n                </div>\r\n                <div className=\"form-group\">\r\n                  <label htmlFor=\"section\">Section</label>\r\n                  <input\r\n                    onChange={(e) => setSection(e.target.value)}\r\n                    type=\"section\"\r\n                    className=\"form-control\"\r\n                    id=\"section\"\r\n                    value={section}\r\n                  />\r\n                </div>\r\n                {contentId === \"\" ? (\r\n                  <button\r\n                    className=\"btn btn-primary mb-2\"\r\n                    type=\"submit\"\r\n                    data-toggle=\"collapse\"\r\n                    data-target=\"#contentControl\"\r\n                    aria-expanded=\"true\"\r\n                    aria-controls=\"contentControl\"\r\n                  >\r\n                    Add\r\n                  </button>\r\n                ) : (\r\n                  <button\r\n                    className=\"btn btn-primary mb-2\"\r\n                    type=\"submit\"\r\n                    data-toggle=\"collapse\"\r\n                    data-target=\"#contentControl\"\r\n                    aria-expanded=\"true\"\r\n                    aria-controls=\"contentControl\"\r\n                  >\r\n                    Update\r\n                  </button>\r\n                )}\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </form>\r\n\r\n        <table className=\"table table-striped table-dark\">\r\n          <thead>\r\n            <tr>\r\n              <th scope=\"col\">No#</th>\r\n              <th scope=\"col\">Content Title</th>\r\n              <th scope=\"col\">Section</th>\r\n              <th>Actions</th>\r\n            </tr>\r\n          </thead>\r\n          <tbody>\r\n            {loading ?<tr><Loader wd={100} hg={100}/></tr> :<>\r\n            {content &&\r\n              content.map((conten) => (\r\n                <tr key={conten._id}>\r\n                  <td>{conten._id}</td>\r\n                  <td>{conten.title}</td>\r\n                  <td>{conten.section}</td>\r\n                  <td>\r\n                    <FaEdit\r\n                      data-toggle=\"collapse\"\r\n                      data-target=\"#contentControl\"\r\n                      aria-expanded=\"true\"\r\n                      aria-controls=\"contentControl\"\r\n                      className=\"text-primary\"\r\n                      style={{ cursor: \"pointer\" }}\r\n                      onClick={() => {\r\n                        setTitle(conten.title);\r\n                        setSection(conten.section);\r\n                        setContentId(conten._id);\r\n                      }}\r\n                    />\r\n                    <FaTrash\r\n                      onClick={() => dispatch(deleteContent(conten._id))}\r\n                      style={{ cursor: \"pointer\" }}\r\n                      className=\"text-danger ml-4\"\r\n                    />\r\n                    <Link to={`/adminEbook/details/${conten._id}`}>\r\n                      <FaExternalLinkAlt\r\n                        className=\"text-light ml-4\"\r\n                        style={{ cursor: \"pointer\" }}\r\n                      />\r\n                    </Link>\r\n                  </td>\r\n                </tr>\r\n              ))}\r\n            </>}\r\n            \r\n          </tbody>\r\n        </table>\r\n      </div>\r\n    </div>\r\n  );\r\n};\r\n\r\nexport default AddminContentScreen;\r\n","D:\\React Project\\UpdatedProject\\frontend\\src\\constants\\eShopConstants\\userConstants.js",[],"D:\\React Project\\UpdatedProject\\frontend\\src\\constants\\eShopConstants\\puchaseConstants.js",[],"D:\\React Project\\UpdatedProject\\frontend\\src\\components\\eBookComponents\\NavEbook.js",[],"D:\\React Project\\UpdatedProject\\frontend\\src\\screens\\eShopScreens\\ProductListScreen.js",[],"D:\\React Project\\UpdatedProject\\frontend\\src\\screens\\adminUsers\\UserDetails.js",[],"D:\\React Project\\UpdatedProject\\frontend\\src\\components\\eShopComponents\\Message.js",[],"D:\\React Project\\UpdatedProject\\frontend\\src\\screens\\eShopScreens\\SaleByPassScreen.jsx",[],"D:\\React Project\\UpdatedProject\\frontend\\src\\screens\\eShopScreens\\PuchaseScreen.jsx",["717","718"],"import React, { useState, useEffect } from \"react\";\r\n\r\n