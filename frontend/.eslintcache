abled={product.countInStock === 0}\r\n                    >\r\n                      Add To Cart\r\n                    </Button>\r\n                    \r\n                  </ListGroup.Item>\r\n                </ListGroup>\r\n              </Card>\r\n            </Col>\r\n          </Row>\r\n          <Row>\r\n            <Col md={6}>\r\n              <h2>Reviews</h2>\r\n              {product.reviews.length === 0 && <Message>No Reviews</Message>}\r\n              <ListGroup variant='flush'>\r\n                {product.reviews.map((review) => (\r\n                  <ListGroup.Item key={review._id}>\r\n                    <strong>{review.name}</strong>\r\n                    <Rating value={review.rating} />\r\n                    <p>{review.createdAt.substring(0, 10)}</p>\r\n                    <p>{review.comment}</p>\r\n                  </ListGroup.Item>\r\n                ))}\r\n                <ListGroup.Item>\r\n                  <h2>Write a Customer Review</h2>\r\n                  {errorProductReview && (\r\n                    <Message variant='danger'>{errorProductReview}</Message>\r\n                  )}\r\n                  {userInfo ? (\r\n                    <Form onSubmit={submitHandler}>\r\n                      <Form.Group controlId='rating'>\r\n                        <Form.Label>Rating</Form.Label>\r\n                        <Form.Control\r\n                          as='select'\r\n                          value={rating}\r\n                          onChange={(e) => setRating(e.target.value)}\r\n                        >\r\n                          <option value=''>Select...</option>\r\n                          <option value='1'>1 - Poor</option>\r\n                          <option value='2'>2 - Fair</option>\r\n                          <option value='3'>3 - Good</option>\r\n                          <option value='4'>4 - Very Good</option>\r\n                          <option value='5'>5 - Excellent</option>\r\n                        </Form.Control>\r\n                      </Form.Group>\r\n                      <Form.Group controlId='comment'>\r\n                        <Form.Label>Comment</Form.Label>\r\n                        <Form.Control\r\n                          as='textarea'\r\n                          row='3'\r\n                          value={comment}\r\n                          onChange={(e) => setComment(e.target.value)}\r\n                        ></Form.Control>\r\n                      </Form.Group>\r\n                      <Button type='submit' variant='primary'>\r\n                        Submit\r\n                      </Button>\r\n                    </Form>\r\n                  ) : (\r\n                    <Message>\r\n                      Please <Link to='/login'>sign in</Link> to write a review{' '}\r\n                    </Message>\r\n                  )}\r\n                </ListGroup.Item>\r\n              </ListGroup>\r\n            </Col>\r\n          </Row>\r\n        </>\r\n      )}\r\n    </>\r\n  )\r\n}\r\n\r\nexport default ProductScreen\r\n","D:\\React Project\\UpdatedProject\\frontend\\src\\actions\\navbarActions.js",[],"D:\\React Project\\UpdatedProject\\frontend\\src\\screens\\eBookScreens\\EbookHomeScreen.jsx",["555","556","557"],"D:\\React Project\\UpdatedProject\\frontend\\src\\components\\NavDash.js",[],"D:\\React Project\\UpdatedProject\\frontend\\src\\components\\Message.js",[],"D:\\React Project\\UpdatedProject\\frontend\\src\\screens\\adminElearn\\AdminCourses.js",[],"D:\\React Project\\UpdatedProject\\frontend\\src\\screens\\eShopScreens\\UserEditScreen.js",[],"D:\\React Project\\UpdatedProject\\frontend\\src\\screens\\eShopScreens\\UserListScreen.js",[],"D:\\React Project\\UpdatedProject\\frontend\\src\\constants\\eShopConstants\\orderConstants.js",[],"D:\\React Project\\UpdatedProject\\frontend\\src\\constants\\eShopConstants\\productConstants.js",[],"D:\\React Project\\UpdatedProject\\frontend\\src\\components\\covideComponents\\Table.js",[],"D:\\