st clearInput = () => {\r\n    setAction(\"add\");\r\n    setProduct(\"Choose a product\");\r\n    setDate(Date.now());\r\n    setQuantity(0);\r\n    setPrice(\"0\");\r\n    setArrived(false);\r\n    setDescription(\"\");\r\n    setPuchaseId(\"\");\r\n    setSupplier(\"\");\r\n  };\r\n  const handleCreatePuchase = (e) => {\r\n    e.preventDefault();\r\n    if (action === \"add\") {\r\n      dispatch(\r\n        createPuchase(\r\n          product,\r\n          date,\r\n          arrived,\r\n          price,\r\n          quantity,\r\n          description,\r\n          supplier\r\n        )\r\n      );\r\n    } else {\r\n      dispatch(\r\n        updatePuchase(\r\n          product,\r\n          date,\r\n          arrived,\r\n          price,\r\n          quantity,\r\n          description,\r\n          supplier,\r\n          puchaseId\r\n        )\r\n      );\r\n    }\r\n    clearInput();\r\n  };\r\n  return (\r\n    <div className=\"bg-warning p-2\">\r\n      <h1>Puchases</h1>\r\n      <Form>\r\n        <Form.Group as={Row} controlId=\"formHorizontalEmail\">\r\n          <Form.Label column sm={2}>\r\n            Product Name\r\n          </Form.Label>\r\n          <Col sm={4}>\r\n            <Form.Control\r\n              as=\"select\"\r\n              size=\"md\"\r\n              value={product}\r\n              onChange={(e) => setProduct(e.target.value)}\r\n              custom\r\n            >\r\n              <option value={product}>{product}</option>\r\n              {products &&\r\n                products.map((product) => (\r\n                  <option key={product.name} value={product.name}>\r\n                    {product.name}\r\n                  </option>\r\n                ))}\r\n            </Form.Control>\r\n          </Col>\r\n          <Form.Label column sm={2}>\r\n            Quatity and Amount\r\n          </Form.Label>\r\n          <Col sm={2}>\r\n            <Form.Control\r\n              type=\"number\"\r\n              placeholder=\"Quantity\"\r\n              value={quantity}\r\n              onChange={(e) => setQuantity(e.target.value)}\r\n            />\r\n          </Col>\r\n          <Col sm={2}>\r\n            <Form.Control\r\n              disabled\r\n              type=\"number\"\r\n              placeholder=\"Amount\"\r\n              value={price * quantity}\r\n            />\r\n          </Col>\r\n        </Form.Group>\r\n        <Form.Group as={Row} controlId=\"formHorizontalEmail\">\r\n          <Form.Label column sm={2}>\r\n            Puchase Date\r\n          </Form.Label>\r\n          <Col sm={4}>\r\n            <Form.Control\r\n              type=\"date\"\r\n              placeholder=\"Date\"\r\n              value={date}\r\n              onChange={(e) => setDate(e.target.value)}\r\n            />\r\n          </Col>\r\n          <Form.Label column sm={2}>\r\n            Supplier\r\n          </Form.Label>\r\n          <Col sm={4}>\r\n            <Form.Control\r\n              type=\"text\"\r\n              value={supplier}\r\n              placeholder=\"Supplier\"\r\n              onChange={(e) => setSupplier(e.target.value)}\r\n            />\r\n          </Col>\r\n        </Form.Group>\r\n        {/* // */}\r\n        <Form.Group as={Row} controlId=\"formHorizontalEmail\">\r\n          <Form.Label column sm={2}>\r\n            Unit Price\r\n          </Form.Label>\r\n          <Col sm={4}>\r\n            <Form.Control\r\n              type=\"number\"\r\n              value={price}\r\n              placeholder=\"Unit Price\"\r\n              onChange={(e) => setPrice(e.target.value)}\r\n            />\r\n          </Col>\r\n          <Form.Label column sm={2}>\r\n            Description\r\n          </Form.Label>\r\n          <Col sm={4}>\r\n            <Form.Control\r\n              type=\"text\"\r\n              value={description}\r\n              onChange={(e) => setDescription(e.target.value)}\r\n              placeholder=\"Description\"\r\n            />\r\n          </Col>\r